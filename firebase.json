{
  "hosting": {
    
      "redirects": [
        {
          "source": "/",
          "destination": "https://www.tomflyons.com/",
          "type": 301
        }
      ],
    
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
